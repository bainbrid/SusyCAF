#!/bin/bash

export SCRAM_ARCH=slc5_amd64_gcc462
CMSSW=CMSSW_5_2_5

cmsrel $CMSSW
cd $CMSSW/src
cmsenv

# susypat (https://twiki.cern.ch/twiki/bin/viewauth/CMS/SusyPatLayer1DefV12)
addpkg DataFormats/PatCandidates   V06-05-01
addpkg PhysicsTools/PatAlgos       V08-09-06
addpkg CommonTools/ParticleFlow    V00-03-11

addpkg PhysicsTools/Configuration  V00-12-02


#MET filters (https://twiki.cern.ch/twiki/bin/view/CMS/MissingETOptionalFilters)
cvs co -r V00-00-01 RecoMET/METFilters

# functions to compute electron IDs
cvs co -r CutBasedId_V00-00-02 -d EGamma/EGammaAnalysisTools UserCode/EGamma/EGammaAnalysisTools

#cvs co -dSUSYBSMAnalysis/SusyCAF UserCode/SusyCAF
cvs co -rV17-pre4 -dSUSYBSMAnalysis/SusyCAF UserCode/SusyCAF


## TODO ##
# taus (https://twiki.cern.ch/twiki/bin/view/CMS/SusyPatLayer1DefV12)
# consider conversion module
# store multiple rhos
# check severity level adjustments
